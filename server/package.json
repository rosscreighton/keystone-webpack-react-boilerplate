{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:prod": "./node_modules/.bin/pm2 start dist/index.js --name 'server'",
    "restart:prod": "./node_modules/.bin/pm2 restart server",
    "start:dev": "yarn run build:watch && ./node_modules/.bin/nodemon ./dist/index.js",
    "build": "./node_modules/.bin/babel ./src --copy-files --out-dir ./dist",
    "build:watch": "yarn run build -w &",
    "provision-server": "DEBUG=http babel-node ./bin/provision_server.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "dotenv": "^4.0.0",
    "keystone": "^4.0.0-beta.5",
    "pm2": "^2.7.0",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "doapi": "^0.1.4",
    "node-ssh": "^4.2.3",
    "nodemon": "^1.12.0"
  }
}
